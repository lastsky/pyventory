{
    "All": {
        "children": [
            "BackEnd",
            "FrontEnd",
            "Staging"
        ],
        "vars": {
            "minify": false,
            "redis_host": "localhost",
            "run_tests": false,
            "use_redis": false,
            "version": "develop"
        }
    },
    "BackEnd": {
        "hosts": [
            "develop",
            "develop_sidebranch",
            "staging"
        ],
        "vars": {
            "use_redis": true
        }
    },
    "FrontEnd": {
        "hosts": [
            "develop",
            "develop_sidebranch",
            "staging"
        ]
    },
    "Staging": {
        "hosts": [
            "develop",
            "develop_sidebranch",
            "staging"
        ],
        "vars": {
            "run_tests": true
        }
    },
    "_meta": {
        "hostvars": {
            "develop": {
                "ansible_host": "develop_hostname",
                "version": "develop"
            },
            "develop_sidebranch": {
                "ansible_host": "sidebranch_hostname",
                "version": "sidebranch_name"
            },
            "staging": {
                "ansible_host": "master_hostname",
                "version": "master"
            }
        }
    }
}